  "scripts": {
    "build": "nest build",
    "format": "prettier --write \\\\\\\"src/**/*.ts\\\\\\\" \\\\\\\"test/**/*.ts\\\\\\\"",
    "start": "npm run start:prod",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main --max-old-space-size=256",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "doc:serve": "npx @compodoc/compodoc -p tsconfig.json -s",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:users": "jest --config src/app/account/users/test/e2e/jest-e2e.json",
    "test:signup": "jest --config src/app/account/auth/test/e2e/jest-e2e.json",
    "test:genre": "jest --config src/app/library/borrow/test/e2e/jest-e2e.json",
    "test:login": "jest --config src/app/account/auth/test/login/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.844.0",
